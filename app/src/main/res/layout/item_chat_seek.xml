<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:contentDescription="@null"
        android:layout_width="@dimen/_14sdp"
        android:layout_height="@dimen/_14sdp"
        android:layout_alignTop="@id/tv_seek"
        android:layout_alignBottom="@id/tv_seek"
        android:adjustViewBounds="true"
        android:src="@drawable/ic_check"
        android:tint="@color/grey_med_dark"
        android:layout_toStartOf="@id/tv_seek"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_seek"
        android:layout_marginTop="@dimen/_2sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:layout_marginBottom="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_3sdp"
        android:layout_alignParentEnd="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/grey_med_dark"
        style="@style/TextAppearance.AppCompat.Small"
        app:fontFamily="@font/nunito_bold"
        android:text="Dilihat"
        tools:ignore="HardcodedText" />

</RelativeLayout>